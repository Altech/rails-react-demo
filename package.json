    "@types/react-router-redux": "^5.0.8",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/webpack-env": "^1.13.1",
    "aphrodite": "^1.2.4",
    "autoprefixer": "7.1.2",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "hypernova": "^2.1.3",
    "immutable": "^3.8.2",
    "jest": "20.0.4",
    "normalizr": "^3.2.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "qhistory": "^1.0.0",
    "react": "^16.0.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-with-styles": "^2.2.1",
    "react-with-styles-interface-aphrodite": "^3.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "node frontend/scripts/start.js",
    "build": "node frontend/scripts/build.js",
    "test": "node frontend/scripts/test.js --env=jsdom",
    "hypernova": "nodemon hypernova.ts",
    "hypernova:prod": "ts-node --fast hypernova.ts"
  },
  "jest": {
    "rootDir": "frontend/",
    "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}"],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|tsx|js|jsx)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|tsx|js|jsx)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/../node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/../node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "ts",
      "tsx"
    ]
  },
  "babel": {
    "presets": ["react-app"]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:3000/",
  "devDependencies": {
    "nodemon": "^1.12.1"
  }
}
